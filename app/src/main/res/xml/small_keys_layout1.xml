<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="@dimen/sizeOfGapInSmall"
    android:keyHeight="50dip"
    android:keyWidth="@dimen/sizeOfKeyInLSmallView"
    android:verticalGap="@dimen/sizeOfGapInSmall">
    <Row>
        <Key
            android:codes="113"
            android:keyEdgeFlags="left"
            android:keyLabel="q" />
        <Key
            android:codes="119"
            android:keyLabel="w" />
        <Key
            android:codes="101"
            android:keyLabel="e"
            android:popupKeyboard="@xml/e_popup"/>
        <Key
            android:codes="114"
            android:keyLabel="r" />
        <Key
            android:codes="116"
            android:keyLabel="t" />
        <Key
            android:codes="121"
            android:keyLabel="y" />
        <Key
            android:codes="117"
            android:keyLabel="u" />
        <Key
            android:codes="105"
            android:keyLabel="i" />
        <Key
            android:codes="111"
            android:keyLabel="o"
            android:popupKeyboard="@xml/o_popup"/>
        <Key
            android:codes="112"
            android:keyEdgeFlags="right"
            android:keyLabel="p" />
    </Row>

    <Row>
        <Key
            android:codes="97"
            android:horizontalGap="5%p"
            android:keyEdgeFlags="left"
            android:keyLabel="a"
            android:popupKeyboard="@xml/a_popup"/>
        <Key
            android:codes="115"
            android:keyLabel="s"
            android:popupKeyboard="@xml/s_popup"/>
        <Key
            android:codes="100"
            android:keyLabel="d" />
        <Key
            android:codes="102"
            android:keyLabel="f" />
        <Key
            android:codes="103"
            android:keyLabel="g" />
        <Key
            android:codes="104"
            android:keyLabel="h" />
        <Key
            android:codes="106"
            android:keyLabel="j" />
        <Key
            android:codes="107"
            android:keyLabel="k" />
        <Key
            android:codes="108"
            android:keyEdgeFlags="right"
            android:keyLabel="l"
            android:popupKeyboard="@xml/l_popup"/>
    </Row>

    <Row>
        <Key
            android:codes="-1"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/up_arrow"
            android:keyWidth="14%p" />
        <Key
            android:codes="122"
            android:keyLabel="z"
            android:popupKeyboard="@xml/z_popup"/>
        <Key
            android:codes="120"
            android:keyLabel="x" />
        <Key
            android:codes="99"
            android:keyLabel="c"
            android:popupKeyboard="@xml/c_popup"/>
        <Key
            android:codes="118"
            android:keyLabel="v" />
        <Key
            android:codes="98"
            android:keyLabel="b" />
        <Key
            android:codes="110"
            android:keyLabel="n"
            android:popupKeyboard="@xml/n_popup"/>
        <Key
            android:codes="109"
            android:keyLabel="m" />
        <Key
            android:codes="-5"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyLabel="DEL"
            android:keyWidth="14%p" />
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key android:horizontalGap="@dimen/sizeOfGapInBottomRowInLSmallView" android:keyWidth="12%p" android:codes="-11" android:keyIcon="@drawable/left_arrow" android:keyEdgeFlags="left"/>
        <Key android:horizontalGap="@dimen/sizeOfGapInBottomRowInLSmallView" android:keyWidth="12%p" android:codes="-10" android:keyIcon="@drawable/right_arrow"/>
        <Key
            android:horizontalGap="1px"
            android:codes="44"
            android:keyLabel=","
            android:keyWidth="12%p" />

        <Key
            android:horizontalGap="1px"
            android:codes="32"
            android:isRepeatable="true"
            android:keyLabel="SPACE"
            android:keyWidth="34%p" />
        <Key
            android:horizontalGap="1px"
            android:codes="46"
            android:keyLabel="."
            android:keyWidth="12%p" />
        <Key
            android:horizontalGap="1px"
            android:codes="10"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/enter"
            android:keyWidth="17%p" />
    </Row>
</Keyboard>
